
public class Warehouse {//extends AbstractClass {
	private int id;
	private int x;
	private int y;
	private int qty[]; //quantity
	
	public Warehouse(int id, int x, int y, int nrProduse) {
		this.setId(id);
		qty=new int[nrProduse];
		this.setX(x);
		this.setY(y);		
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int[] getQty() {
		return qty;
	}
	public void setQty(int[] qty) {
		this.qty = qty;
	}
	
	public void setProdus(int j, int cantitate){
		this.qty[j]=cantitate;
	}

	
}
